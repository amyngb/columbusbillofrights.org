<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title | Columbus Community Bill of Rights</title>
    <meta name="description" content="" />
    <meta name="Author" content="JMG Software" />
    <meta name="viewport" content="width=device-width, maximum-scale=1, initial-scale=1, user-scalable=0" />
    <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600%7CRaleway:300,400,500,600,700%7CLato:300,400,400italic,600,700" rel="stylesheet" type="text/css" />
    @Styles.Render("~/Content/bootstrap")
    @Styles.Render("~/Content/core")
    @if (IsSectionDefined("AddToHead"))
    {@RenderSection("AddToHead", required: false)}
</head>
<body class="smoothscroll enable-animation">
    @Html.Partial("_SlideTop")
    <div id="wrapper">
        @Html.Partial("_TopBar")
        @Html.Partial("_Header")
        <div>
            @RenderBody()
        </div>
        @Html.Partial("_Footer")
    </div>
    <a href="#" id="toTop"></a>
    <div id="preloader">
        <div class="inner">
            <span class="loader"></span>
        </div>
    </div>
    <div>
        @Html.Partial("~/Views/Shared/_ShowWaitModal.cshtml")
    </div>
    <script>
        var plugin_path = '@Url.Content("~/Content/assets/plugins/")';
         var urlCommon = {
            SubscribeToNewsletter: '@Url.Action("SubscribeToNewsletter", "Home")'
        };
    </script>
    @Scripts.Render("~/bundles/jQueryCore")
    @Scripts.Render("~/bundles/jQueryFeatures")
    @Scripts.Render("~/bundles/siteScripts")
    @RenderSection("scripts", required: false)
</body>
</html>